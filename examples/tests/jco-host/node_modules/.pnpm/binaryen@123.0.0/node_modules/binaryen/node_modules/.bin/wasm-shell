#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mz/ghq/github.com/mizchi/wit-bindgen-moonbit/examples/tests/jco-host/node_modules/.pnpm/binaryen@123.0.0/node_modules/binaryen/bin/node_modules:/Users/mz/ghq/github.com/mizchi/wit-bindgen-moonbit/examples/tests/jco-host/node_modules/.pnpm/binaryen@123.0.0/node_modules/binaryen/node_modules:/Users/mz/ghq/github.com/mizchi/wit-bindgen-moonbit/examples/tests/jco-host/node_modules/.pnpm/binaryen@123.0.0/node_modules:/Users/mz/ghq/github.com/mizchi/wit-bindgen-moonbit/examples/tests/jco-host/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mz/ghq/github.com/mizchi/wit-bindgen-moonbit/examples/tests/jco-host/node_modules/.pnpm/binaryen@123.0.0/node_modules/binaryen/bin/node_modules:/Users/mz/ghq/github.com/mizchi/wit-bindgen-moonbit/examples/tests/jco-host/node_modules/.pnpm/binaryen@123.0.0/node_modules/binaryen/node_modules:/Users/mz/ghq/github.com/mizchi/wit-bindgen-moonbit/examples/tests/jco-host/node_modules/.pnpm/binaryen@123.0.0/node_modules:/Users/mz/ghq/github.com/mizchi/wit-bindgen-moonbit/examples/tests/jco-host/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/wasm-shell" "$@"
else
  exec node  "$basedir/../../bin/wasm-shell" "$@"
fi
