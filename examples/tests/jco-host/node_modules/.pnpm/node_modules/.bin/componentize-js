#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mz/ghq/github.com/mizchi/wit-bindgen-moonbit/examples/tests/jco-host/node_modules/.pnpm/@bytecodealliance+componentize-js@0.19.3/node_modules/@bytecodealliance/componentize-js/src/node_modules:/Users/mz/ghq/github.com/mizchi/wit-bindgen-moonbit/examples/tests/jco-host/node_modules/.pnpm/@bytecodealliance+componentize-js@0.19.3/node_modules/@bytecodealliance/componentize-js/node_modules:/Users/mz/ghq/github.com/mizchi/wit-bindgen-moonbit/examples/tests/jco-host/node_modules/.pnpm/@bytecodealliance+componentize-js@0.19.3/node_modules/@bytecodealliance/node_modules:/Users/mz/ghq/github.com/mizchi/wit-bindgen-moonbit/examples/tests/jco-host/node_modules/.pnpm/@bytecodealliance+componentize-js@0.19.3/node_modules:/Users/mz/ghq/github.com/mizchi/wit-bindgen-moonbit/examples/tests/jco-host/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mz/ghq/github.com/mizchi/wit-bindgen-moonbit/examples/tests/jco-host/node_modules/.pnpm/@bytecodealliance+componentize-js@0.19.3/node_modules/@bytecodealliance/componentize-js/src/node_modules:/Users/mz/ghq/github.com/mizchi/wit-bindgen-moonbit/examples/tests/jco-host/node_modules/.pnpm/@bytecodealliance+componentize-js@0.19.3/node_modules/@bytecodealliance/componentize-js/node_modules:/Users/mz/ghq/github.com/mizchi/wit-bindgen-moonbit/examples/tests/jco-host/node_modules/.pnpm/@bytecodealliance+componentize-js@0.19.3/node_modules/@bytecodealliance/node_modules:/Users/mz/ghq/github.com/mizchi/wit-bindgen-moonbit/examples/tests/jco-host/node_modules/.pnpm/@bytecodealliance+componentize-js@0.19.3/node_modules:/Users/mz/ghq/github.com/mizchi/wit-bindgen-moonbit/examples/tests/jco-host/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@bytecodealliance/componentize-js/src/cli.js" "$@"
else
  exec node  "$basedir/../@bytecodealliance/componentize-js/src/cli.js" "$@"
fi
