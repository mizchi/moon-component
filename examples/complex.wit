package my:complex;

interface types {
  record point {
    x: s32,
    y: s32,
  }

  variant shape {
    circle(s32),
    rectangle(point),
    none,
  }

  greet: func(name: string) -> string;
  add-points: func(a: point, b: point) -> point;
  area: func(s: shape) -> option<s32>;
  parse-int: func(s: string) -> result<s32, string>;
  get-items: func() -> list<point>;
}

world example {
  export types;
}
